{
  "name": "eoneopay/bank-files",
  "description": "EoneoPay Bank files parsing and dumping from LoyaltyCorp.",
  "keywords": ["eoneopay", "bank files", "loyaltycorp"],
  "license": "MIT",
  "type": "package",
  "require": {
    "php": ">=7.1",
    "tightenco/collect": "^5.5",
    "roave/security-advisories": "dev-master",
    "eoneopay/utils": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.2",
    "mockery/mockery": "^1.0",
    "symfony/var-dumper": "^4.0"
  },
  "autoload": {
    "psr-4": { "EoneoPay\\BankFiles\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": { "Tests\\EoneoPay\\BankFiles\\": "tests/" }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "optimize-autoloader": true,
  "repositories": [
    {
      "type": "vcs",
      "url": "git@bitbucket.org:eoneopay/utils.git"
    }
  ]
}
